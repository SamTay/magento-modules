<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:blueacorn:framework-entity-map:etc/entity_decode.xsd">
    <entity name="catalog_product">
        <key_map from="somethingCamelCased" to="something_snake_cased" />
        <key_map from="anotherExample" to="another_example" />
        <key_map from="spColor" to="color" />
        <aggregate id="confidence_builder">
            <key id="free_shipping_all_orders" />
            <key id="money_back_guarantee" />
        </aggregate>
        <attribute_map code="color" mapper="BlueAcorn\ProductIntegration\Model\Mapper\Attribute\Color" />
        <attribute_map code="confidence_builder" mapper="BlueAcorn\ProductIntegration\Model\Mapper\Attribute\ConfidenceBuilder" />
    </entity>
</config>
